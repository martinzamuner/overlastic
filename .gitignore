# Ignore gem bundle
*.gem

# Ignore lock files
Gemfile.lock

# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/test/dummy/log/*
/test/dummy/tmp/*
!/test/dummy/log/.keep
!/test/dummy/tmp/.keep

# Ignore pidfiles, but keep the directory.
/test/dummy/tmp/pids/*
!/test/dummy/tmp/pids/
!/test/dummy/tmp/pids/.keep

/public/assets
mise.toml
.byebug_history
TAGS

/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity
